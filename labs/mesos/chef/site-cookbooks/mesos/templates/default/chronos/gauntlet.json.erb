{
  "command": "curl -X DELETE -L -H \"Content-Type: application/json\" http://<%= @master_ip %>:8080/v2/apps/producer && cd <%= @gauntlet_install_dir %> && ./gradlew jar && ./validate.sh --kafka.source.topic dataset --kafka.destination.topic mirror_dataset --kafka.fetch.size 64 --kafka.partitions 1",
